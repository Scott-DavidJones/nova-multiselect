{
    "name": "autumndev/nova-multiselect",
    "description": "A Laravel Nova multiselect field.",
    "keywords": [
        "laravel",
        "nova",
        "selectize"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Autumndev\\Nova\\MultiSelect\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Autumndev\\Nova\\MultiSelect\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
